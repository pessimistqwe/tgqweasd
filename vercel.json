{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/events",
            "dest": "api/index.py"
        },
        {
            "src": "/user/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/sync/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/index.html"
        }
    ]
}
