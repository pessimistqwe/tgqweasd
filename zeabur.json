{
  "name": "eventpredict",
  "description": "Telegram Mini App Prediction Market",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "envs": {
    "PORT": "8000",
    "TEST_DB_PATH": "/tmp/events.db",
    "PYTHONPATH": "/app:/app/api",
    "PYTHONUNBUFFERED": "1",
    "DISABLE_SCHEDULER": "0",
    "POLYMARKET_SYNC_INTERVAL": "7200"
  }
}
